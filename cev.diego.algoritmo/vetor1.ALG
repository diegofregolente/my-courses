algoritmo "vetor1"
var
   n: vetor[1..4] de inteiro:
   i: inteiro
inicio
      n[1] <- 4
      escreval(n[1])
      n[2] <- 3
      escreval(n[2])
      n[3] <- 5
      escreval(n[3])
      n[4] <- 1
      escreval(n[4])
      para i <- 1 ate 4 passo 1 faca
          escreva(n[i])
      fimpara
fimalgoritmo