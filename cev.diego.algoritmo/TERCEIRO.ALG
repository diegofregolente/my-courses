algoritmo "TERCEIRO"
var
 N1, N2, soma: Inteiro
 media: real
inicio
      escreval("Digite o numero 1:")
      leia(N1)
      escreval("Digite o numero2:")
      leia(N2)
      soma <- (N1 + N2)
      media <- (N1 + N2) / 2
      escreval("A soma entre os numeros é ", soma)
      escreval("A média é ", media)
fimalgoritmo
